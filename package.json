{
  "name": "backend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@codemirror/lang-javascript": "^6.2.2",
    "@codemirror/lang-python": "^6.1.6",
    "@codemirror/state": "^6.4.1",
    "@codemirror/view": "^6.34.3",
    "@monaco-editor/react": "4.4.3",
    "@uiw/react-codemirror": "^4.23.6",
    "axios": "^1.7.8",
    "cors": "^2.8.5",
    "deploy-node-app": "^1.4.3",
    "dotenv": "^8.6.0",
    "express": "^4.21.1",
    "katex": "^0.16.11",
    "nvm": "^0.0.4",
    "openai": "^4.72.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-markdown": "6",
    "react-scripts": "3.0.1",
    "rehype-katex": "5",
    "remark-math": "3",
    "serve": "^14.2.4",
    "vfile": "5"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "concurrently": "^4.1.0",
    "cross-spawn": "^6.0.5",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1"
  },
  "scripts": {
    "start": "node src/api/index.js",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && react-scripts build",
    "build-www": "cd src/www && yarn build",
    "heroku-postbuild": "yarn build-www",
    "start-www": "npx serve -s build"
  },
  "engines": {
    "node": "18.x",
    "npm": "8.x"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "deploy-node-app": {
    "prod": {
      "port": 4000,
      "protocol": "http",
      "entrypoint": "src/api/index.js",
      "type": "spa"
    },
    "dev": {
      "port": 4000,
      "protocol": "http",
      "entrypoint": "src/api/index.js",
      "registry": "",
      "type": "spa",
      "isPublic": false
    }
  },
  "proxy": "http://localhost:4000"
}
